    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "fetch-intercept": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rilog-development/rilog-lib.git"
